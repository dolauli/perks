{
  "name": "@azure-tools/codegen",
  "version": "3.0.0",
  "patchOffset": 100,
  "description": "Autorest Code generator common and base classes",
  "engines": {
    "node": ">=12.13.0"
  },
  "directories": {
    "doc": "docs"
  },
  "main": "dist/exports.js",
  "typings": "dist/exports.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "prepare": "npm run build",
    "test": "npm run build && mocha dist/test --timeout=1000000",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/perks.git"
  },
  "keywords": [
    "AutoRest",
    "codegen"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/perks/issues"
  },
  "homepage": "https://github.com/Azure/perks/tree/master/codegen#readme",
  "readme": "https://github.com/Azure/perks/tree/master/codegen/readme.md",
  "devDependencies": {
    "@types/js-yaml": "3.12.1",
    "@types/mocha": "~7.0.2",
    "@types/node": "12.7.2",
    "mocha": "7.1.2",
    "@testdeck/mocha": "~0.1.0",
    "typescript": "~3.9.5",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "@types/semver": "5.5.0",
    "source-map-support": "0.5.13"
  },
  "dependencies": {
    "@azure-tools/async-io": "~4.0.0",
    "@azure-tools/linq": "~4.0.0",
    "js-yaml": "3.13.1",
    "semver": "^5.5.1"
  }
}